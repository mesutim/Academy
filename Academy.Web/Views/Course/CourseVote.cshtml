@model Tuple<int,int>

@if (User.Identity.IsAuthenticated && ViewBag.NotAccess==null)
{

    <div class="row">
        <img src="/images/votew.jpg"/>
        <p></p>
        <p>
            <a class="btn btn-success" onclick="vote(true)">
                <img class="btnLike" src="/images/like.png"/>
                میپسندم (@Model.Item1)
            </a>
            <a class="btn btn-danger" onclick="vote(false)">
                <img class="btnLike" src="/images/dislike.png" />
                نمی پسندم (@Model.Item2)
            </a>
        </p>
    </div>
}